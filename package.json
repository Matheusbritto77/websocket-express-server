{
    "name": "websocket-express-server",
    "version": "0.1.0",
    "description": "A project with Vue 3 + Vite frontend and Express backend",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run dev --prefix frontend\" \"npm run server --prefix backend\"",
        "dev": "vite --prefix frontend",
        "build": "vite build --prefix frontend",
        "serve": "vite preview --prefix frontend",
        "server": "node backend/server.js"
    },
    "dependencies": {
        "express": "^4.17.1",
        "vue": "^3.0.0"
    },
    "devDependencies": {
        "concurrently": "^7.6.0",
        "vite": "^4.0.0"
    },
    "keywords": [
        "vue",
        "vite",
        "express",
        "backend",
        "frontend"
    ],
    "author": "Seu Nome",
    "license": "MIT"
}
